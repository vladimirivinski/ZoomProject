<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Call_Duration__c</fullName>
    <description>Call duration in mm:ss format from seconds</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(Inbound_Call_Duration__c),null,
 

IF((MOD((Inbound_Call_Duration__c)/60,1)*60)  &gt;=  10,
 

TEXT(FLOOR( (Inbound_Call_Duration__c)/60)) + &quot;h : &quot; + TEXT( FLOOR(MOD((Inbound_Call_Duration__c)/60,1)*60)) +&quot;min&quot;,
 

TEXT(FLOOR( (Inbound_Call_Duration__c)/60)) + &quot;h : 0&quot; + TEXT( FLOOR(MOD((Inbound_Call_Duration__c)/60,1)*60)) +&quot;min&quot;
 

))</formula>
    <label>Call Duration (Inbound)</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
